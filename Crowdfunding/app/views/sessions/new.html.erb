<div class="login vertical-center-row" align="center">
  <form action="/login" class="" method="post">
    <input type="hidden" name="authenticity_token" value="" />

    <label for="">Email</label>
    <input type="text" name="[email]" value="" placeholder="Email" class="form-control" /><br>

    <label for="">Password</label>
    <input type="password" name="[password]" value="" placeholder="Password" class="form-control" /> <br>

    <button type="submit" class="btn btn-success btn-block" name="commit" value="submit">Submit</button> <br>
    <a href="/users/new">Sign Up</a>
  </form>

</div>
