    <div class="dropdown">
      <button class="btn btn-default dropdown-toggle color-button-home" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
        <%=@name%>
        <span class="caret"></span>
      </button>
        <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
          <li  align="center"></li>
          <li role="separator" class="divider",></li>
          <li align="center" class="color-li-menu"><%= link_to "Post", "/post", class:"redirect-menu"%></li>
          <li role="separator" class="divider",></li>
          <li align="center" class="color-li-menu"><%= link_to "Config", "/config",class:"redirect-menu"%></li>
          <li role="separator" class="divider",></li>
